{
    "extends": ["plugin:react/recommended"],
    "env": {
        "es6": true
    },
    "rules": {
        "arrow-parens": "error",
        "react/jsx-uses-vars": ["warn"],
        "no-unused-vars": "warn",
        "react/jsx-uses-vars": "warn",
        "react/jsx-uses-react": "warn"
    },
    "parserOptions": {
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "modules": true
        }
    }
}
